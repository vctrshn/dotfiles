// Place your settings in this file to overwrite the default settings
{
    "ActiveFileInStatusBar.fullpath": false,
    "css.lint.duplicateProperties": "warning",
    "dateTime.padHours": false,
    "dateTime.showAMPM": true,
    "dateTime.showDayOfMonth": true,
    "dateTime.showDayOfWeek": true,
    "dateTime.showMonth": true,
    "dateTime.use24HourClock": false,
    "debug.inlineValues": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.autoIndent": true,
    "editor.cursorBlinking": "solid",
    "editor.dragAndDrop": true,
    "editor.emptySelectionClipboard": false,
    "editor.fontFamily": "Inconsolata for Powerline",
    "editor.fontLigatures": true,
    "editor.fontSize": 10,
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": true
    },
    "editor.minimap.enabled": false,
    "editor.renderIndentGuides": true,
    "editor.renderWhitespace": "all",
    "editor.rulers": [80],
    "editor.stablePeek": true,
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "emmet.useNewEmmet": true,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "emmet.syntaxProfiles": {
        "jsx": {
            "attr_quotes": "single"
        }
    },
    "emmet.showExpandedAbbreviation": "always",
    "eslint.run": "onSave",
    "extensions.autoUpdate": false,
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "*.min.js": true,
        "*.min.css": true,
        "**/dist-*": true,
        "**/coverage": true,
        "**/_maps": true,
        "*.neomake.log": true,
        "**/vendor": true,
        "**/.gen": true,
        "**/.tmp": true,
        "**/go-build": true
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "flow.runOnEdit": false,
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.blame.line.enabled": false,
    "go.useCodeSnippetsOnFunctionSuggest": false,
    "go.vetFlags": [],
    "javascript.format.enable": false,
    "javascript.validate.enable": false,
    "material-icon-theme.showUpdateMessage": false,
    "npm-intellisense.importLinebreak": ";\n",
    "npm-intellisense.scanDevDependencies": true,
    "path-intellisense.showHiddenFiles": true,
    "prettier.singleQuote": true,
    "prettier.trailingComma": "es5",
    "prettier.parser": "flow",
    "search.useRipgrep": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.lineHeight": 1.5,
    "terminal.integrated.fontSize": 10,
    "typescript.npm": "/usr/local/bin/npm",
    "vim.easymotion": true,
    "vim.easymotionMarkerBackgroundColor": "#222",
    "vim.easymotionMarkerFontFamily": "Inconsolata for Powerline",
    "vim.easymotionMarkerFontSize": "10",
    "vim.easymotionMarkerFontWeight": "600",
    "vim.easymotionMarkerHeight": 10,
    "vim.easymotionMarkerYOffset": 8,
    "vim.enableNeovim": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": ["j", "k"],
            "after": ["<Esc>"]
        },
        {
            "before": [";", ";"],
            "after": ["<Esc>", "A", ";", "<Esc>"]
        },
        {
            "before": [",", ","],
            "after": ["<Esc>", "A", ",", "<Esc>"]
        }
    ],
    "vim.otherModesKeyBindingsNonRecursive": [
        {
            "before": [";"],
            "after": [":"]
        },
        {
            "before": ["j"],
            "after": ["g", "j"]
        },
        {
            "before": ["k"],
            "after": ["g", "k"]
        }
    ],
    "vim.leader": " ",
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.timeout": 200,
    "window.restoreWindows": "all",
    "window.openFilesInNewWindow": "off",
    "workbench.colorTheme": "Material Theme",
    "workbench.editor.closeOnFileDelete": false,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.tips.enabled": false,
    "workbench.startupEditor": "none",
    "window.zoomLevel": 0,
    "vim.disableAnnoyingNeovimMessage": true
}
